{
    "name": "Know Myself",
    "description": "Knowledge Management System - Chrome Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Click to view MyndGraph",
        "default_popup": "popup.html"
    },
    "permissions": [
        "readingList",
        "tabs",
        "activeTab",
        "tabCapture",
        "contextMenus",
        "bookmarks",
        "storage",
        "sidePanel"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    }
}