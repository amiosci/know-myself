{
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}/.venv/lib/python3.11/site-packages",
    ],
    "python.analysis.extraPaths": [
        "${workspaceFolder}/.venv/lib/python3.11/site-packages"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.venv": true,
        "**/__pycache__": true,
        "**/node_modules": true,
        "**/.nyc_output": true
    },
    "python.analysis.typeCheckingMode": "basic"
}